{
  "name": "fcc-exercise-tracker",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jake Klassen",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "shx rm -rf dist",
    "prebuild": "pnpm run clean",
    "build": "node --loader tsm src/build.ts",
    "build:only": "node --loader tsm src/build.ts",
    "compile-schemas": "json2ts -i src/schemas -o src/types",
    "lint": "eslint .",
    "premigrate": "pnpm run build",
    "migrate": "node --env-file=.env dist/migrate.js",
    "watch": "tsc --watch",
    "start": "nodemon",
    "migrate:test": "node --env-file=.env.test dist/migrate.js up",
    "pretest": "run-s build migrate:test",
    "test": "node --env-file=.env.test node_modules/mocha/bin/_mocha",
    "prod": "node ./dist",
    "typecheck": "tsc"
  },
  "imports": {
    "#app/*": [
      "./dist/*"
    ]
  },
  "dependencies": {
    "@sinclair/typebox": "^0.33.5",
    "async-cleanup": "^1.0.0",
    "awilix": "^10.0.2",
    "bson": "^6.8.0",
    "change-case": "^5.4.4",
    "comment-json": "^4.2.5",
    "convict": "^6.2.4",
    "fastify": "^4.28.1",
    "fp-ts": "^2.16.9",
    "http-status-codes": "^2.3.0",
    "io-ts": "^2.2.21",
    "json-schema-to-typescript": "14.0.4",
    "mongodb": "^6.8.0",
    "mongoose": "8.5.3",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "pino": "9.3.2",
    "pino-http": "^10.2.0",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.2.2",
    "tslog": "^4.9.3",
    "typescript": "^5.5.4",
    "umzug": "^3.8.1"
  },
  "devDependencies": {
    "@ngneat/falso": "7.2.0",
    "@types/convict": "6.1.6",
    "@types/mocha": "10.0.7",
    "@types/node": "22.3.0",
    "@types/wtfnode": "0.7.3",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "clipboardy": "4.0.0",
    "esbuild": "0.23.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "expect": "29.7.0",
    "get-port": "7.1.0",
    "get-port-cli": "3.0.0",
    "mocha": "10.7.3",
    "nodemon": "3.1.4",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "shx": "0.3.4",
    "tiny-glob": "0.2.9",
    "ts-node": "10.9.2",
    "tsm": "2.3.0",
    "wtfnode": "0.9.3"
  }
}
