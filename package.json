{
	"name": "fcc-exercise-tracker",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "Jake Klassen",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"clean": "shx rm -rf dist",
		"prebuild": "pnpm run clean",
		"build": "node --loader tsm src/build.ts",
		"build:only": "node --loader tsm src/build.ts",
		"compile-schemas": "json2ts -i src/schemas -o src/types",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" eslint.config.mjs",
		"lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\" eslint.config.mjs",
		"premigrate": "pnpm run build",
		"migrate": "node --env-file=.env dist/migrate.js",
		"watch": "tsc --watch",
		"start": "nodemon",
		"migrate:test": "node --env-file=.env.test dist/migrate.js up",
		"pretest": "run-s build migrate:test",
		"test": "node --env-file=.env.test node_modules/mocha/bin/_mocha",
		"prod": "node ./dist",
		"typecheck": "tsc"
	},
	"imports": {
		"#app/*": [
			"./dist/*"
		]
	},
	"dependencies": {
		"@sinclair/typebox": "^0.34.14",
		"async-cleanup": "^1.0.0",
		"awilix": "^12.0.4",
		"bson": "^6.10.1",
		"change-case": "^5.4.4",
		"comment-json": "^4.2.5",
		"convict": "^6.2.4",
		"fastify": "^4.28.1",
		"fp-ts": "^2.16.9",
		"http-status-codes": "^2.3.0",
		"io-ts": "^2.2.22",
		"json-schema-to-typescript": "15.0.4",
		"mongodb": "^6.12.0",
		"mongoose": "8.9.5",
		"monocle-ts": "^2.3.13",
		"newtype-ts": "^0.3.5",
		"pino": "9.6.0",
		"pino-http": "^10.4.0",
		"pino-pretty": "^13.0.0",
		"reflect-metadata": "^0.2.2",
		"tslog": "^4.9.3",
		"typescript": "^5.7.3",
		"umzug": "^3.8.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@ngneat/falso": "7.3.0",
		"@types/convict": "6.1.6",
		"@types/eslint__eslintrc": "^2.1.2",
		"@types/eslint__js": "^8.42.3",
		"@types/mocha": "10.0.10",
		"@types/node": "22.10.7",
		"@types/wtfnode": "0.7.3",
		"@typescript-eslint/eslint-plugin": "8.21.0",
		"@typescript-eslint/parser": "8.21.0",
		"clipboardy": "4.0.0",
		"esbuild": "0.24.2",
		"eslint": "9.18.0",
		"eslint-config-prettier": "10.0.1",
		"eslint-plugin-depend": "^0.12.0",
		"eslint-plugin-prettier": "5.2.3",
		"expect": "29.7.0",
		"get-port": "7.1.0",
		"get-port-cli": "3.0.0",
		"globals": "^15.14.0",
		"mocha": "11.0.1",
		"nodemon": "3.1.9",
		"npm-run-all2": "5.0.0",
		"prettier": "3.4.2",
		"shx": "0.3.4",
		"tiny-glob": "0.2.9",
		"ts-node": "10.9.2",
		"tsm": "2.3.0",
		"wtfnode": "0.10.0"
	}
}
