{
  "name": "fcc-exercise-tracker",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jake Klassen",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "shx rm -rf dist",
    "prebuild": "pnpm run clean",
    "build": "node --loader tsm src/build.ts",
    "build:only": "node --loader tsm src/build.ts",
    "compile-schemas": "json2ts -i src/schemas -o src/types",
    "lint": "eslint .",
    "premigrate": "pnpm run build",
    "migrate": "node -r dotenv/config dist/migrate.js",
    "watch": "tsc --watch",
    "start": "nodemon",
    "migrate:test": "DOTENV_CONFIG_PATH=.env.test node -r dotenv/config dist/migrate.js up",
    "pretest": "run-s build migrate:test",
    "test": "DOTENV_CONFIG_PATH=.env.test mocha -r dotenv/config",
    "prod": "node ./dist",
    "typecheck": "tsc"
  },
  "imports": {
    "#app/*": [
      "./dist/*"
    ]
  },
  "dependencies": {
    "@sinclair/typebox": "^0.24.5",
    "awilix": "^7.0.3",
    "bson": "^4.6.4",
    "change-case": "^4.1.2",
    "comment-json": "^4.2.2",
    "convict": "^6.2.3",
    "fastify": "^4.2.0",
    "fp-ts": "^2.12.1",
    "http-status-codes": "^2.2.0",
    "io-ts": "^2.2.16",
    "json-schema-to-typescript": "11.0.1",
    "mongodb": "^4.7.0",
    "mongoose": "6.4.2",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "pino": "^8.1.0",
    "pino-http": "^8.1.0",
    "pino-pretty": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.7.4",
    "umzug": "^3.1.1"
  },
  "devDependencies": {
    "@ngneat/falso": "6.0.2",
    "@types/convict": "6.1.1",
    "@types/mocha": "9.1.1",
    "@types/node": "18.0.0",
    "@types/wtfnode": "0.7.0",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "clipboardy": "3.0.0",
    "dotenv": "16.0.1",
    "esbuild": "0.14.48",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "expect": "28.1.1",
    "get-port": "6.1.2",
    "get-port-cli": "3.0.0",
    "mocha": "10.0.0",
    "nodemon": "2.0.18",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "shx": "0.3.4",
    "tiny-glob": "0.2.9",
    "ts-node": "10.8.1",
    "tsm": "2.2.1",
    "wtfnode": "0.9.1"
  }
}
