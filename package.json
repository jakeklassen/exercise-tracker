{
  "name": "fcc-exercise-tracker",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jake Klassen",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "shx rm -rf dist",
    "prebuild": "pnpm run clean",
    "build": "node --loader tsm src/build.ts",
    "build:only": "node --loader tsm src/build.ts",
    "compile-schemas": "json2ts -i src/schemas -o src/types",
    "lint": "eslint .",
    "premigrate": "pnpm run build",
    "migrate": "node -r dotenv/config dist/migrate.js",
    "watch": "tsc --watch",
    "start": "nodemon",
    "migrate:test": "DOTENV_CONFIG_PATH=.env.test node -r dotenv/config dist/migrate.js up",
    "pretest": "run-s build migrate:test",
    "test": "DOTENV_CONFIG_PATH=.env.test mocha -r dotenv/config",
    "prod": "node ./dist",
    "typecheck": "tsc"
  },
  "imports": {
    "#app/*": [
      "./dist/*"
    ]
  },
  "dependencies": {
    "@sinclair/typebox": "^0.31.1",
    "async-cleanup": "^1.0.0",
    "awilix": "^8.0.1",
    "bson": "^5.4.0",
    "change-case": "^4.1.2",
    "comment-json": "^4.2.3",
    "convict": "^6.2.4",
    "fastify": "^4.21.0",
    "fp-ts": "^2.16.1",
    "http-status-codes": "^2.2.0",
    "io-ts": "^2.2.20",
    "json-schema-to-typescript": "13.0.2",
    "mongodb": "^5.7.0",
    "mongoose": "7.4.3",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "pino": "8.15.0",
    "pino-http": "^8.4.0",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "tslog": "^4.9.1",
    "typescript": "^5.1.6",
    "umzug": "^3.3.1"
  },
  "devDependencies": {
    "@ngneat/falso": "7.1.0",
    "@types/convict": "6.1.3",
    "@types/mocha": "10.0.1",
    "@types/node": "20.5.0",
    "@types/wtfnode": "0.7.0",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "clipboardy": "3.0.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.2",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "expect": "29.6.2",
    "get-port": "7.0.0",
    "get-port-cli": "3.0.0",
    "mocha": "10.2.0",
    "nodemon": "3.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.1",
    "shx": "0.3.4",
    "tiny-glob": "0.2.9",
    "ts-node": "10.9.1",
    "tsm": "2.3.0",
    "wtfnode": "0.9.1"
  }
}
